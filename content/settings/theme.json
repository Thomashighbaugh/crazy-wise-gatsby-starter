{
  "color": {
    "black": "#24262d",
    "white": "#F5F8FA",
    "primary": "#1DA1F2",
    "secondary": "#555e70"
  },
  "easing": "cubic-bezier(0.215, 0.610, 0.355, 1.000)",
  "breakpoints": {
    "small": "600px",
    "medium": "1200px",
    "large": "1600px",
    "huge": "2200px"
  },
  "radius": {
    "small": "12px"
  },
  "header": {
    "overline": true,
    "transparent": true,
    "height": "4.5rem",
    "underline": true
  },
  "hero": {
    "image": "../images/5.png",
    "large": false,
    "overlay": true,
    "overlap": "10rem",
    "parallax": true
  },
  "typography": {
    "uppercaseH2": true
  },
  "menu": {
    "style": "glow"
  },
  "__gatsby_resolved": {
    "fileRelativePath": "/content/settings/theme.json"
  }
}