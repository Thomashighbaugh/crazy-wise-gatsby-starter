{
  "title": "Contact",
  "path": "/contact",
  "hero": {
    "headline": "Contact Our Staff",
    "textline": "Sign up for an exclusive newletter sent to your inbox, send us some feedback about the blog or even just drop us a line.",
    "image": "../images/LPC5.jpg",
    "large": false
  },
  "blocks": [
    {
      "_template": "TitleBlock",
      "title": "",
      "underline": true,
      "content": ""
    },
    {
      "_template": "FormBlock",
      "name": "Contact Form",
      "fields": [
        {
          "label": "Name",
          "inputType": "text",
          "autocomplete": "name",
          "_template": "nameInputBlock"
        },
        {
          "label": "Email",
          "inputType": "text",
          "autocomplete": "email",
          "_template": "emailInputBlock"
        },
        {
          "label": "Message",
          "inputType": "textarea",
          "autocomplete": "",
          "_template": "messageInputBlock"
        }
      ],
      "content": ""
    }
  ],
  "displayTitle": true
}
